<h1>HelpList</h1>
<h2>Helping Humans Help Humans</h2>
<%= form_with url: "/login", method: :get do |form| %>
  <%= form.label :email, "Email:"%>
  <%= form.text_field :email %>
  <%= form.label :password, "Password:" %>
  <%= form.password_field :password %>
  <%= form.submit "Login" %> 
<% end %>
<%= button_to "Login with Google", "/auth/google_oauth2"  %>

<p>Don't have an account? <%= link_to "Register Here", "/register" %> </p>

<%= link_to  "Do you or someone you know need help?" %>
